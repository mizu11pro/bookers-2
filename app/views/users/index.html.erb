<%= flash[:notice] %>

<%= render 'books/list', book: @book, user: current_user %>

<h1>Users</h1>
<h2>image</h2>
<h2>name</h2>

<% @users.each do |f| %>
<%= attachment_image_tag f, :profile_image, :fill, 50, 50, format: 'jpeg', fallback: "no_image.jpg" %>
<%= f.name %>
<%= link_to "Show", user_path(f.id) %>
<% end %>