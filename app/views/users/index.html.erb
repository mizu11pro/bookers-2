<%= render 'books/list', book: @book, user: current_user %>

<div class="col-md-8 offset-md-1">
<h2>Users</h2>
    <table class="table">
        <thead>
            <tr>
                <th>image</th>
                <th>name</th>
                <th col-span="3"></th>
            </tr>
        </thead>
        <tbody>
        <% @users.each do |f| %>
            <tr>
                <td>
                  <%= attachment_image_tag f, :profile_image, size: "40x40", format: 'jpeg', fallback: "no_image.jpg" %>
                </td>
                <td><%= f.name %></td>
                <td><%= link_to "Show", user_path(f.id) %></td>
            </tr>
        <% end %>
        </tbody>
    </table>
</div>

